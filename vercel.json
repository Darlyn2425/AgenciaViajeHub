{
    "cleanUrls": true,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/",
            "dest": "/App/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/App/$1"
        }
    ]
}